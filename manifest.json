{
  "manifest_version": 2,

  "name": "iwantoknowme",
  "description": "Find out your browsing habits",
  "version": "1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "tabs",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": ["bower_components/lodash/lodash.min.js", "util.js", "db.js", "data-collector.js"]
  }
}