{
  "manifest_version": 2,

  "name": "iwantoknowm",
  "description": "Find out your browsing habits",
  "version": "1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "tabs",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": ["data-collector.js"]
  }
}